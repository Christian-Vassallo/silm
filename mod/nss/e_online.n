extern("inc_events")
extern("inc_online")

void main() {
	struct EventInfo ei = GetEventInfo();

	if ("module_load" == ei.ev) {
		onlineServerStart();
	}

	if ("player_hb" == ei.ev) {
		onlinePlayerUpdateLocation(ei.runnable);
	}

	if ("player_login" == ei.ev) {
		onlinePlayerEnter(ei.actor);
	}
	
	if ("player_logout" == ei.ev) {
		onlinePlayerLeave(ei.actor);
	}

}
