extern("inc_events")
extern("inc_online")

void main() {
	struct EventInfo ei = GetEventInfo();

	if ("module_load" == ei.ev) {
		onlineServerStart();
	}

	if ("module_hb" == ei.ev) {
		onlinePlayerUpdateLocation(ei.actor);
	}

	if ("pc_login" == ei.ev) {
		onlinePlayerEnter(ei.actor);
	}
	
	if ("pc_logout" == ei.ev) {
		onlinePlayerLeave(ei.actor);
	}

}
